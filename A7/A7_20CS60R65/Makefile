all:
	( cat ./chatLogs/day1.txt | python3 mapper1.py  | sort -n | python3 combiner1.py && cat ./chatLogs/day2.txt | python3 mapper1.py  | sort -n | python3 combiner1.py &&  cat ./chatLogs/day3.txt | python3 mapper1.py  | sort -n | python3 combiner1.py &&  cat ./chatLogs/day4.txt | python3 mapper1.py  | sort -n | python3 combiner1.py &&  cat ./chatLogs/day5.txt | python3 mapper1.py  | sort -n | python3 combiner1.py &&   cat ./chatLogs/day6.txt | python3 mapper1.py  | sort -n | python3 combiner1.py &&  cat ./chatLogs/day7.txt | python3 mapper1.py  | sort -n | python3 combiner1.py && cat ./chatLogs/day8.txt | python3 mapper1.py  | sort -n | python3 combiner1.py &&  cat ./chatLogs/day9.txt | python3 mapper1.py  | sort -n | python3 combiner1.py && cat ./chatLogs/day10.txt  | python3 mapper1.py  | sort -n | python3 combiner1.py  ) | sort -n -k1 -k2 | python3 reducer1.py > network.txt
	python3 mapper2.py | sort -n -k1 -k2 -k3 | python3 reducer2.py > friends.txt
	python3 mapper3.py | sort -n -k1 -k2 -k3 | python3 reducer3.py > triangles.txt